    "borderRadiusLaptop": {
      "type": "object"
    },
    "borderRadiusWideScreen": {
      "type": "object"
    },
    "hoverColor": {
      "type": "string"
    },
    "bgHoverColor": {
      "type": "object"
    },
    "hoverBoxShadow": {
      "type": "object"
    },
    "hoverBorderDesktop": {
      "type": "object"
    },
    "hoverBorderTablet": {
      "type": "object"
    },
    "hoverBorderMobile": {
      "type": "object"
    },
    "hoverBorderTabletLandscape": {
      "type": "object"
    },
    "hoverBorderMobileLandscape": {
      "type": "object"
    },
    "hoverBorderLaptop": {
      "type": "object"
    },
    "hoverBorderWideScreen": {
      "type": "object"
    },
    "hoverBorderRadiusDesktop": {
      "type": "object"
    },
    "hoverBorderRadiusTablet": {
      "type": "object"
    },
    "hoverBorderRadiusMobile": {
      "type": "object"
    },
    "hoverBorderRadiusTabletLandscape": {
