      "type": "string",
      "default": "#FF5050"
    },
    "lineBgColor": {
      "type": "string",
      "default": "#EEEEEE"
    },
    "textColor": {
      "type": "string"
    },
    "bgColor": {
      "type": "object"
    },
    "boxShadow": {
      "type": "object"
    },
    "borderDesktop": {
      "type": "object"
    },
    "borderTablet": {
      "type": "object"
    },
    "borderMobile": {
      "type": "object"
    },
    "borderTabletLandscape": {
      "type": "object"
    },
    "borderMobileLandscape": {
      "type": "object"
    },
    "borderLaptop": {
      "type": "object"
    },
    "borderWideScreen": {
      "type": "object"
    },
    "borderRadiusDesktop": {
      "type": "object",
      "default": {
        "top": "50px",
        "right": "50px",
        "bottom": "50px",
        "left": "50px"
